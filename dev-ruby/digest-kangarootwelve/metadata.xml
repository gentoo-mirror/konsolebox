<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pkgmetadata SYSTEM "https://www.gentoo.org/dtd/metadata.dtd">
<pkgmetadata>
	<maintainer type="person">
		<email>konsolebox@gmail.com</email>
		<name>konsolebox</name>
		<description>author</description>
	</maintainer>
	<longdescription>
		A KangarooTwelve library that utilizes the XKCP package and can be optionally optimized
		during compile-time based on a target. It produces implementation classes that work on top
		of Digest::Base and conform with how Digest classes work.
	</longdescription>
	<upstream>
		<remote-id type="github">konsolebox/digest-kangarootwelve-ruby</remote-id>
		<bugs-to>https://github.com/konsolebox/digest-kangarootwelve-ruby/issues</bugs-to>
	</upstream>
	<use>
		<flag name="target_armv6">Build 'armv6' target</flag>
		<flag name="target_armv6m">Build 'armv6m' target</flag>
		<flag name="target_armv7a">Build 'armv7a' target</flag>
		<flag name="target_armv7m">Build 'armv7m' target</flag>
		<flag name="target_armv8a">Build 'armv8a' target</flag>
		<flag name="target_avr8">Build 'avr8' target</flag>
		<flag name="target_avx">Build 'avx' target</flag>
		<flag name="target_avx2">Build 'avx2' target</flag>
		<flag name="target_avx2noasm">Build 'avx2noasm' target</flag>
		<flag name="target_avx512">Build 'avx512' target</flag>
		<flag name="target_avx512noasm">Build 'avx512noasm' target</flag>
		<flag name="target_compact">Build 'compact' target</flag>
		<flag name="target_generic32">Build 'generic32' target</flag>
		<flag name="target_generic32lc">Build 'generic32lc' target</flag>
		<flag name="target_generic64">Build 'generic64' target</flag>
		<flag name="target_generic64lc">Build 'generic64lc' target</flag>
		<flag name="target_ssse3">Build 'ssse3' target</flag>
		<flag name="target_xop">Build 'xop' target</flag>
		<flag name="test">Run tests. This may require FEATURES="test".</flag>
	</use>
</pkgmetadata>
